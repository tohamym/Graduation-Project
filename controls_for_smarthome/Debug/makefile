-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
LSS += \
controls_for_smarthome.lss \

FLASH_IMAGE += \
controls_for_smarthome.hex \

SIZEDUMMY += \
sizedummy \


# All Target
all: controls_for_smarthome.elf secondary-outputs

# Tool invocations
controls_for_smarthome.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: AVR C Linker'
	avr-gcc -Wl,-Map,controls_for_smarthome.map -mmcu=atmega32 -o "controls_for_smarthome.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

controls_for_smarthome.lss: controls_for_smarthome.elf
	@echo 'Invoking: AVR Create Extended Listing'
	-avr-objdump -h -S controls_for_smarthome.elf  >"controls_for_smarthome.lss"
	@echo 'Finished building: $@'
	@echo ' '

controls_for_smarthome.hex: controls_for_smarthome.elf
	@echo 'Create Flash image (ihex format)'
	-avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex controls_for_smarthome.elf  "controls_for_smarthome.hex"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: controls_for_smarthome.elf
	@echo 'Invoking: Print Size'
	-avr-size --format=avr --mcu=atmega32 controls_for_smarthome.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(FLASH_IMAGE)$(ELFS)$(OBJS)$(ASM_DEPS)$(S_DEPS)$(SIZEDUMMY)$(S_UPPER_DEPS)$(LSS)$(C_DEPS) controls_for_smarthome.elf
	-@echo ' '

secondary-outputs: $(LSS) $(FLASH_IMAGE) $(SIZEDUMMY)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
